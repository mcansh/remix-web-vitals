{
  "name": "@mcansh/remix-web-vitals",
  "version": "0.1.0",
  "description": "Web Vitals for Remix",
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint --cache --ignore-path .gitignore --fix .",
    "format": "prettier --write --ignore-path .gitignore .",
    "publint": "publint",
    "validate": "run-p build lint format publint",
    "test": "pnpm run validate"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": "mcansh/remix-web-vitals",
  "keywords": [
    "remix",
    "performance",
    "metrics",
    "vitals",
    "web-vitals",
    "webvitals",
    "cls",
    "fcp",
    "fid",
    "lcp",
    "ttfb"
  ],
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh/)",
  "license": "MIT",
  "devDependencies": {
    "@remix-run/eslint-config": "^1.17.1",
    "@tsconfig/node16": "^1.0.4",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "eslint": "^8.43.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "publint": "^0.1.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.13.0",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "web-vitals": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^17.0.2 | ^18.0.0",
    "react-dom": "^17.0.2 | ^18.0.0",
    "react-router-dom": "^6.11.2"
  }
}
