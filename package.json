{
  "name": "@mcansh/remix-web-vitals",
  "version": "0.3.5",
  "description": "Web Vitals for Remix",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint --cache --ignore-path .gitignore --fix .",
    "format": "prettier --write --ignore-path .gitignore .",
    "publint": "publint",
    "validate": "run-p build lint format publint",
    "test": "pnpm run validate",
    "changeset": "changeset",
    "changeset:version": "changeset version && node ./scripts/remove-prerelease-changelogs.js",
    "changeset:release": "pnpm run build && changeset publish"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "import": "./dist/index.d.ts",
        "default": "./dist/index.d.cts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/mcansh"
    }
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": "mcansh/remix-web-vitals",
  "keywords": [
    "remix",
    "performance",
    "metrics",
    "vitals",
    "web-vitals",
    "webvitals",
    "cls",
    "fcp",
    "fid",
    "lcp",
    "ttfb"
  ],
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh/)",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@manypkg/get-packages": "^2.2.0",
    "@remix-run/eslint-config": "^2.0.0",
    "@remix-run/react": "^2.0.0",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.49.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "publint": "^0.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "web-vitals": "^3.4.0"
  },
  "peerDependencies": {
    "@remix-run/react": "^1.17.1 || ^2.0.0",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
