{
  "name": "@mcansh/remix-web-vitals",
  "version": "0.3.2",
  "description": "Web Vitals for Remix",
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint --cache --ignore-path .gitignore --fix .",
    "format": "prettier --write --ignore-path .gitignore .",
    "publint": "publint",
    "validate": "run-p build lint format publint",
    "test": "pnpm run validate",
    "changeset": "changeset",
    "changeset:version": "changeset version && node ./scripts/remove-prerelease-changelogs.mjs",
    "changeset:release": "pnpm run build && changeset publish"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": "mcansh/remix-web-vitals",
  "keywords": [
    "remix",
    "performance",
    "metrics",
    "vitals",
    "web-vitals",
    "webvitals",
    "cls",
    "fcp",
    "fid",
    "lcp",
    "ttfb"
  ],
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh/)",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@remix-run/eslint-config": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "@tsconfig/node16": "^16.1.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.45.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "publint": "^0.1.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "web-vitals": "^3.4.0"
  },
  "peerDependencies": {
    "@remix-run/react": "^1.17.1",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  }
}
